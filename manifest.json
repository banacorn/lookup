{
    "manifest_version": 2,

    "name": "Lookup",
    "description": "Lookup articles in wiktionary",
    "version": "1.0",
    "devtools_page": "dist/devtools.html",
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*",
        "file://*/*"
    ],
    "page_action": {
        "default_title": "Lookup"
    },
    "background": {
        "scripts": ["dist/background.js"],
        "persistent": false
    }
}
