{
    "manifest_version": 2,

    "name": "Lookup",
    "description": "Lookup articles in wiktionary",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "tabs",
        "storage"
    ],

    "page_action": {
        "default_title": "Lookup"
    },

    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["js/parser.js", "js/parser/pronunciation.js", "js/f.js", "js/content.js", "bower_components/lodash/index.js"]
    }],
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },


    "options_ui": {
        "page": "html/options.html"
    }
}
