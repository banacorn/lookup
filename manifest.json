{
    "manifest_version": 2,

    "name": "Wörterbuch",
    "description": "Wörterbuch",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "tabs"
    ],

    "page_action": {
        "default_title": "Wörterbuch"
    },

    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["parser.js", "content.js"]
    }],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}
